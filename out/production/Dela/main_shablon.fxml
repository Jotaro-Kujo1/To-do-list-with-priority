            <Font size="17.0" />
         </font>
      </TextField>
      <Button fx:id="addButton" layoutX="21.0" layoutY="345.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="116.0" style="-fx-background-color: #FF6347;" text="Добавить" />
