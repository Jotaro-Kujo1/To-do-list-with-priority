        <columns>
          <TableColumn fx:id="prepodColumn" prefWidth="112.0" text="Препод" />
          <TableColumn fx:id="workColumn" prefWidth="113.0" text="Задание" />
          <TableColumn fx:id="numsColumn" prefWidth="79.0" text="Количество" />
